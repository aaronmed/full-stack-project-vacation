type User {
	id: ID!,
	username: String,
	password: String,
	name: String,
	surname: String,
	dni: String,
	telephone: String
}

type Query {
	users: [User],
	user(id: ID): User
	
}

type Mutation {
	createUser(username: String, password: String, name: String, surname: String, dni: String, telephone: String): User
	
}

